#include <bits/stdc++.h>
using namespace std;
int main()
{
  int n;
  cin>>n;
  int v=0;
  v=n%2;
   if (v!=0) 
   {
   cout<<"Weird";
   } 
   else if (v==0 && n>=2 && n<=5) {
   cout<<"Not Weird";
   }
   else if (v==0 && 6>=2 && n<=20) {
   cout<<"Weird";
   }
   else {
   cout<<"Not Weird";
   }
   return 0;
}
